    <script>
      var head = document.head;
      var script = document.createElement('script');
      script.type = 'text/javascript';
      script.src =
        'https://t.plinthsny.com/v1/lst/universal-script?ph=ec8b190bff1954e67156656a8053268832f885bf8261e96c411fa325bc55a599&tag=!clicked&origin=SHOPIFY&ref_url=' +
        encodeURI(document.URL);
      head.appendChild(script);
