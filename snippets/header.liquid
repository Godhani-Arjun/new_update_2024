{{siteHeader | strip_newlines | remove: "  " | remove: "	" }}

<style>
.header-txt.middle-hdr.text-center {
	text-transform: capitalize;
	font-weight: 500 !important;
	display: flex;
	align-items: center;
	margin-top: 1px;
	letter-spacing: -0.2px;
}
.top-header .social-icons{    margin: 0;}
.top-header .left-hdr a {
	padding: 0;
	margin-right: 20px;
}
.top-header .left-hdr .ad {
	margin-right: 0;
}
.site-header {
    max-width: 1760px;
}	
#siteNav>li>a{margin-right: 0;letter-spacing: 0.1px;}
#siteNav>li>a.active:after {
    content: "";
    position: absolute;
    width: 6px;
    height: 6px;
    border-radius: 3px;
    background: #3D4936;
    left: 0;
    right: 0;
    margin: 0 auto;
    bottom: 0;
}
#siteNav>li>a {        padding: 0 25px 16px;    font-weight: 700;}
.header-logo img{        width: 100%;}
@media screen and (min-width: 1025px) {
	.header-txt.middle-hdr.text-center {
		position: absolute;
		margin: 0 auto;
		text-align: center;
		left: 20px;
		right: 20px;
		width: fit-content;
		margin-top: 0;
	}

	
}
@media screen and (max-width: 1366px) {
#siteNav>li>a {	padding: 0 10px 16px;    font-size: 14px;}
}
@media screen and (max-width: 1024px) {
	.top-header {min-height: 40px;}
	.site-header .mobile-nav, .site-header .icons-col {        max-width: fit-content;    }
	.site-header a.hdicon{    padding: 0 10px;}
	#CartCount, .favCount{right: 2px;}

}
@media screen and (max-width: 480px) {
	#CartCount, .favCount {
        min-width: unset;
        right: 2px;
        width: 18px;
        height: 18px;
        color: #FFF;
        text-align: center;
        font-size: 8px;
        font-weight: 500;
        line-height: 10.4px;
        padding-top: 1px;
		padding-right: 0.5px;
    }
	.site-header a.hdicon{margin-top: -6px !important;}
	.site-header .icons-col{    margin-right: -5px !important;}
}
</style>